[Unit]
Description=Monitor Module DSI Service

[Service]
WorkingDirectory=/home/worker/GG31_SYSTEM/opt/service_script
PIDFile=/var/run/module_dsi.pid
ExecStart=/home/worker/GG31_SYSTEM/opt/service_script/service_module_dsi.sh start
ExecStop=/home/worker/GG31_SYSTEM/opt/service_script/service_module_dsi.sh stop

[Install]
WantedBy=multi-user.target
